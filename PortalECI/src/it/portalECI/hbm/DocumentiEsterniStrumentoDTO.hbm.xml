<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="it.portalECI.DTO.DocumentiEsterniStrumentoDTO" table="documenti_esterni">
       
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>

        <property name="nomeDocumento" type="java.lang.String">
            <column name="nome_documento" />
        </property>
         <property name="dataCaricamento" type="java.util.Date">
            <column name="data_caricamento" />
        </property>
    
    		 <property name="id_strumento" type="int">
            <column name="id_strumento" />
        </property>


    </class>
</hibernate-mapping>
