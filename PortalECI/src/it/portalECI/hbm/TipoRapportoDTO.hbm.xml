<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
    <class name="it.portalECI.DTO.TipoRapportoDTO" table="TIPO_RAPPORTO">
        <id name="id" type="int">
            <column name="__ID" />
